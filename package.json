    "jssm4": "^1.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.12.0",
    "sass-resources-loader": "^2.1.0",
    "store": "^2.0.12",
    "typescript": "^4.0.5",
    "umi": "^3.2.16",
    "wangeditor": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5"
  }
