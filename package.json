    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/store": "^2.0.2",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.16",
    "antd": "^4.8.4",
    "axios": "^0.20.0",
    "compression-webpack-plugin": "^6.0.4",
    "crypto-js": "^4.0.0",
    "evian": "^2.0.4",
    "highlight.js": "^10.4.1",
